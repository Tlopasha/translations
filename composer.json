    "type": "antaresproject-component",
    "version": "0.9.2.1",
    "homepage": "https://github.com/antaresproject/translations",    
    "prefer-stable": true,
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Åukasz Cirut",
            "email": "contact@antaresproject.io"
        }
    ],
    "require": {
        "antaresproject/component-installer-plugin": "0.9.2.x-dev",
        "antaresproject/core": "0.9.2.x-dev",
        "antaresproject/component-users": "0.9.2.x-dev",    
        "antaresproject/component-logger": "0.9.2.x-dev",
        "antaresproject/component-installer": "0.9.2.x-dev",
        "laravel/framework": "5.4.*",
        "php": ">=7.0.1"
    },
    "autoload": {
        "psr-4": {
            "Antares\\Translations\\": "src/"
        }
    },
    "extra": {
        "friendly-name": "Translations"
    }
}
